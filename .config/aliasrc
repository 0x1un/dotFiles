# Basic alias
alias ..='cd ..'
alias ...='cd ../../'
alias ....='cd ../../../'
alias ls='ls --color=auto'
alias lc='colorls -lA --sd'
alias vim='nvim'
alias ping='prettyping'
alias bcat='bat'
alias du='ncdu --color dark -rr -x'
alias grep='rg'

# proxy
alias v2='nohup v2ray >> ~/Log/v2ray.log 2>&1 &; echo "v2ray running in background."'

# trash-cli
alias rm='trash-put'
alias rmt='trash-empty'
alias rmm='trash-rm'
alias rml='trash-list'
alias rmr='trash-restore'
